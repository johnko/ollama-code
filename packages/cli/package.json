{
  "name": "@johnko/ollama-code-cli",
  "version": "0.0.1-alpha.8",
  "description": "Ollama Code CLI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tcsenpai/ollama-code.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ollama": "dist/index.js"
  },
  "scripts": {
    "build": "node ../../scripts/build_package.js",
    "start": "node dist/index.js",
    "debug": "node --inspect-brk dist/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "config": {
    "sandboxImageUri": "us-docker.pkg.dev/gemini-code-dev/gemini-cli/sandbox:0.0.1-alpha.8"
  },
  "dependencies": {
    "@johnko/ollama-code": "file:../core",
    "@types/update-notifier": "^6.0.8",
    "command-exists": "^1.2.9",
    "diff": "^7.0.0",
    "dotenv": "^17.1.0",
    "gaxios": "^7.1.1",
    "glob": "^11.0.0",
    "highlight.js": "^11.11.1",
    "ink": "^6.0.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-link": "^4.1.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "lowlight": "^3.3.0",
    "mime-types": "^3.0.1",
    "open": "^11.0.0",
    "react": "^19.1.0",
    "read-package-up": "^12.0.0",
    "shell-quote": "^1.8.3",
    "string-width": "^7.1.0",
    "strip-ansi": "^7.1.0",
    "strip-json-comments": "^5.0.0",
    "update-notifier": "^7.3.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.28.4",
    "@testing-library/react": "16.3.1",
    "@types/command-exists": "1.2.3",
    "@types/diff": "7.0.2",
    "@types/dotenv": "6.1.1",
    "@types/node": "20.19.28",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@types/semver": "7.7.1",
    "@types/shell-quote": "1.7.5",
    "@types/yargs": "17.0.35",
    "ink-testing-library": "4.0.0",
    "jsdom": "26.1.0",
    "pretty-format": "30.2.0",
    "react-dom": "19.2.3",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=20"
  }
}
